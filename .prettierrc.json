{
  "arrowParens": "always",
  "attributeGroups": [
    "$ANGULAR_STRUCTURAL_DIRECTIVE",
    "$ANGULAR_INPUT",
    "$ANGULAR_TWO_WAY_BINDING",
    "$ANGULAR_OUTPUT",
    "$CODE_GUIDE"
  ],
  "bracketSameLine": false,
  "bracketSpacing": true,
  "dataTypeCase": "lower",
  "denseOperators": false,
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "lf",
  "expressionWidth": 50,
  "functionCase": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "identifierCase": "preserve",
  "indentStyle": "standard",
  "jsxSingleQuote": false,
  "keywordCase": "upper",
  "language": "postgresql",
  "linesBetweenQueries": 1,
  "logicalOperatorNewline": "before",
  "multilineArraysWrapThreshold": 3,
  "newlineBeforeSemicolon": false,
  "overrides": [
    {
      "files": [
        ".bash_profile",
        "*.json",
        "*.sh"
      ],
      "options": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": true
      }
    }
  ],
  "plugins": [
    "/opt/homebrew/lib/node_modules/@awmottaz/prettier-plugin-void-html/prettier-plugin-void-html.js",
    "/opt/homebrew/lib/node_modules/prettier-plugin-embed/dist/index.js",
    "/opt/homebrew/lib/node_modules/prettier-plugin-multiline-arrays/dist/index.js",
    "/opt/homebrew/lib/node_modules/prettier-plugin-organize-attributes/lib/index.js",
    "/opt/homebrew/lib/node_modules/prettier-plugin-sh/lib/index.js",
    "/opt/homebrew/lib/node_modules/prettier-plugin-sql/lib/index.js"
  ],
  "printWidth": 140,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "semi": true,
  "singleAttributePerLine": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
